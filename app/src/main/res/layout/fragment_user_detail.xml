<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Activities.Administration.FragmentUserDetail">

    <TextView
        android:id="@+id/user_name_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:text="TextView"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/user_phone_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/user_name_txt"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/user_name_txt"
        app:layout_constraintTop_toBottomOf="@+id/userid_txt" />

    <TextView
        android:id="@+id/user_school_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/user_phone_txt"
        app:layout_constraintStart_toStartOf="@+id/user_phone_txt"
        app:layout_constraintTop_toBottomOf="@+id/user_phone_txt" />

    <TextView
        android:id="@+id/user_grade_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/user_school_txt"
        app:layout_constraintStart_toStartOf="@+id/user_school_txt"
        app:layout_constraintTop_toBottomOf="@+id/user_school_txt" />

    <TextView
        android:id="@+id/user_gender_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/user_grade_txt"
        app:layout_constraintStart_toStartOf="@+id/user_grade_txt"
        app:layout_constraintTop_toBottomOf="@+id/user_grade_txt" />

    <TextView
        android:id="@+id/user_state_city_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/user_gender_txt"
        app:layout_constraintStart_toStartOf="@+id/user_gender_txt"
        app:layout_constraintTop_toBottomOf="@+id/user_gender_txt" />

    <TextView
        android:id="@+id/user_classes_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/user_state_city_txt"
        app:layout_constraintStart_toStartOf="@+id/user_state_city_txt"
        app:layout_constraintTop_toBottomOf="@+id/user_state_city_txt" />

    <TextView
        android:id="@+id/user_classes_created_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/user_classes_txt"
        app:layout_constraintStart_toStartOf="@+id/user_classes_txt"
        app:layout_constraintTop_toBottomOf="@+id/user_classes_txt" />

    <TextView
        android:id="@+id/userid_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/user_name_txt"
        app:layout_constraintStart_toStartOf="@+id/user_name_txt"
        app:layout_constraintTop_toBottomOf="@+id/user_name_txt" />

    <Button
        android:id="@+id/joined_class_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Joined class list"
        app:layout_constraintEnd_toEndOf="@+id/user_classes_created_txt"
        app:layout_constraintStart_toStartOf="@+id/user_classes_created_txt"
        app:layout_constraintTop_toBottomOf="@+id/user_classes_created_txt" />

    <Button
        android:id="@+id/created_class_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Created class list"
        app:layout_constraintEnd_toEndOf="@+id/joined_class_btn"
        app:layout_constraintStart_toStartOf="@+id/joined_class_btn"
        app:layout_constraintTop_toBottomOf="@+id/joined_class_btn" />

</androidx.constraintlayout.widget.ConstraintLayout>